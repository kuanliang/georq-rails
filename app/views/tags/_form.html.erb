<%#simple_form_for service do |f| %>
 <%= simple_form_for tag do |f| %> 
  <div class="form-project">
       <%= f.input :tag_name,  input_html: { class: "form-control"} %>
       <%= f.input :follow,  input_html: { class: "form-control"} %>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submitting..." } %>
<% end %>