<%= simple_form_for form do |f| %>
  <div >  

  	<%= f.input :file, as: :file,label: "空白範例檔" %>
    <%= f.input :name, input_html: { class: "form-control"} %>

    <%= f.input :describe, input_html: { class: "form-control"} %>

    <%= f.input :version, input_html: { class: "form-control"} %>

    <%#= f.input :standard, input_html: { class: "form-control"} %>


    <%= f.input :company, input_html: { class: "form-control"} %>
    
    <%= f.input :follow, input_html: { class: "form-control"} %>
    <%#= f.input :description, input_html: { class: "form-control"} %>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submitting..." } %>
<% end %>
